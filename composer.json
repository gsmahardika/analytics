{
    "name": "gsmahardika/analytics",
    "description": "A package to retrieve Google Analytics data.",
    "keywords": [
        "google",
        "analytics",
        "retrieve",
        "reports"
    ],
    "homepage": "https://github.com/gsmahardika/analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Gs Mahardika",
            "email": "gs.mahardika@gmail.com",
            "homepage": "http://gsmahardika.web.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "google/apiclient": "^2.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.1|^7.0"
    },
    "autoload": {
        "psr-4": {
          "Agung\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agung\\Analytics\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
